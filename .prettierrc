{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "printWidth": 100,
    "trailingComma": "es5",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "importOrder": [
        "^react$",
        "^next",
        "<THIRD_PARTY_MODULES>",
        "^@/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}